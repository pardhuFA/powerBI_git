	measure 'Top 5 Games Stakes' =
			
			VAR Top5Games =
			    TOPN(
			        5,
			        FILTER(
			            SUMMARIZE(
			                ALL('Dim Game Meta'),
			                'Dim Game Meta'[gameName],
			                "Total Stakes Game Stats", [Total Stakes Game Stats]
			            ),
			            [Total Stakes Game Stats] <> BLANK()
			        ),
			        [Total Stakes Game Stats], DESC
			    )
			VAR CurrentGame = SELECTEDVALUE('Dim Game Meta'[gameName])
			RETURN
			    IF(
			        CurrentGame IN SELECTCOLUMNS(Top5Games, "Gamename", 'Dim Game Meta'[gameName]),
			        [Total Stakes Game Stats],
			        BLANK()
			    )
		displayFolder: Base
		lineageTag: 8128ee98-36f8-4c54-9e01-c1758de781c0

